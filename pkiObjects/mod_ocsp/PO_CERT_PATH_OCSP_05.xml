        <SerialNumber>32105</SerialNumber>
        <Extension oid="1.3.6.1.5.5.7.1.1" critical="false" name="Authority Information Access" type="pretty">${subAIA}</Extension>
    </Certificate>
	  
	<OcspResponse id="CERT_PATH_OCSP_05_SUB_CA_RESP" responseStatus="successful">
        <Location>${rootAIA}</Location>
        <Version>0</Version>
        <VerifiedBy>CERT_PATH_OCSP_05_ROOT_CA</VerifiedBy>
        <Signature>${ocsp.signature}</Signature>
        <ResponderId type="byName"></ResponderId>
        <ProducedAt encoding="GEN">-1S</ProducedAt>
        <ResponseEntry refid="CERT_PATH_OCSP_05_SUB_CA"> 
          <HashAlgorithm>${ocsp.idHash}</HashAlgorithm> 
          <CertStatus>
            <Status>good</Status>
          </CertStatus>
          <ThisUpdate encoding="GEN">-1D</ThisUpdate>
          <NextUpdate encoding="GEN">+6D</NextUpdate>
        </ResponseEntry>
    </OcspResponse>
	
	<OcspResponse id="CERT_PATH_OCSP_05_EE_RESP" responseStatus="successful">
        <Location>${subAIA}</Location>
        <Version>0</Version>
        <VerifiedBy>CERT_PATH_OCSP_05_SUB_CA</VerifiedBy>
        <Signature>${ocsp.signature}</Signature>
        <ResponderId type="byName"></ResponderId>
        <ProducedAt encoding="GEN">-1S</ProducedAt>
        <ResponseEntry refid="CERT_PATH_OCSP_05_EE"> 
          <HashAlgorithm>${ocsp.idHash}</HashAlgorithm> 
          <CertStatus>
            <Status>good</Status>
          </CertStatus>
          <ThisUpdate encoding="GEN">-1D</ThisUpdate>
          <NextUpdate encoding="GEN">+6D</NextUpdate>
        </ResponseEntry>
