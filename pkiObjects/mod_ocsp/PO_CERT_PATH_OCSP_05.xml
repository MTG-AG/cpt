    <Certificate id="CERT_PATH_OCSP_05_ROOT_CA" refid="CERT_PATH_COMMON_01_ROOT_CA" type="TA"/>
    <Certificate id="CERT_PATH_OCSP_05_SUB_CA" refid="CERT_PATH_OCSP_01_SUB_CA">
        <VerifiedBy>CERT_PATH_OCSP_05_ROOT_CA</VerifiedBy>
        <SerialNumber>42105</SerialNumber>
        <Extension oid="1.3.6.1.5.5.7.1.1" critical="false" name="Authority Information Access" type="pretty">${rootAIA}</Extension>
    </Certificate>
    <Certificate id="CERT_PATH_OCSP_05_EE" refid="CERT_PATH_COMMON_01_EE" type="TC">
